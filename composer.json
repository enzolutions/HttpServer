{
    "name":"yosymfony/httpserver",
    "description": "A simple HTTP server",
    "keywords": ["http", "server", "request", "react"],
    "license": "MIT",
    "homepage": "http://yosymfony.com",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com"
        }
